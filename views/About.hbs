{{!-- service---------------------------------------------------------- --}}




<section id="services_section" class="py-4 ">
       <div class="container">

         <h1 class="text-center text-success">Services that we provides</h1>

        <div class="row mt-3">
         {{#each services}}
         <div class="col-md-3 ">

          <div class="card ">

            <div class="card-body  text-center ">
               <div class="box">
              <div class="container text-center">
              <i class="{{this.icon}} service-icon"></i>
              </div>
              <h3 class="card-title">{{this.title}}</h3>
              <p>{{this.description}}</p>
              <a class="btn btn-primary" href="{{this.link}}">{{this.linkText}}</a>
              </div>
            </div>
            
          </div>
              
         </div>
         {{/each}}
        </div>
       </div>

       </section>

     {{>footer data3=footer}}

    
